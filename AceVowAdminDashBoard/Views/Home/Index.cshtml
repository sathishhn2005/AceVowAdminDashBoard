    function qrCount(id) {
        $('#exampleModal').modal('show');
        // SetId(id);

        if (id == 0 || id == undefined) {
            id = $('#hdnId').val();
        }
        var objUser = { FromDate: $('#txtFromDate').val(), ToDate: $('#txtToDate').val(), UserId: id };
        $.ajax({
            type: "GET",
            contentType: "application/json; charset=utf-8",
            url: "GetCountQR",
            data: objUser,
            dataType: "json",
            success: function (Result) {
                $('#lblCount').html("Total No.of count " + Result.Result);
                $('#hdnId').val(id);
            },
            error: function (Result) {
                alert("Error");
            }
        });

    }
</script>