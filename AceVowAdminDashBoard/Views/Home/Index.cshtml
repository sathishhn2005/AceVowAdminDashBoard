    });
    function GetClient() {
        var param = { ClientName: $('#txtClientName').val(), StoreUrl: $('#txtStoreUrl').val(), City: $('#txtClientCity').val() };
        $("#tblClient").find("tr:not(:first)").remove();

        $.ajax({

            type: "GET",
            url: "GetClientInfo",
            dataType: "json",
            contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
            data: param,
            success: function (Result) {
                if (Result.length > 0) {
                    var i = 0;
                    $.each(Result, function (key, value) {
                        i++;
                        var hdnID = "hdnobjJSonVal_" + i;
                        var hdId = i;
                        var JVal = JSON.stringify(value);

                        //var v = new Date(parseInt(value.PolicyFromDate.replace("/Date(", "").replace(")/", ""), 10));
                        ////Fromvalue = value.toDateString()
                        //Fromvalue = v.toISOString();

                        //var Str =

                        //    ("00" + value.getDate()).slice(-2)
                        //    + "-" + ("00" + (value.getMonth() + 1)).slice(-2)
                        //    + "-" + value.getFullYear()

                        //$('#PolicyFromDate').val(Str);
                        $('#tblClient').append('<tr> <td><a href="javascript:qrCount(' + value.Id + ');">' + value.StoreName + '</a></td><td>' + value.ContactName + '</td><td>' + value.EmailId + '</td><td>' + value.AddressLine1 + '</td><td><button class="btn btn-primary-login" type="button" data-toggle="modal" onclick=SetId(' + value.Id + ') data-target="#uploadBulk"><i class="mdi mdi-cloud-upload"></i></button></td><td><button class="btn btn-secondary-login" type="button" data-toggle="modal" onclick=SetId(' + value.Id + ') data-target="#CategoryUpdate"><i class="mdi mdi-library-plus"></i></button></td><td class="text-center"><i class="mdi mdi-pencil text-primary mx-3" onclick="EditClientMaster(' + "'" + value.Id + "'" + ')"></i><i class="mdi mdi-delete text-danger mx-3" onclick="DeActiveRIMaster(this,' + value.Id.toString() + ');"></i><input type="hidden" id=' + hdnID + ' value=' + "'" + JVal + "'" + '></td></tr>');


                    })
                }
                else {
                    $('#tblClient').append('<tr> <td colspan="14" aligen="center" > No Records found </td></tr>');
                }
                //$("#tblRIMaster tr").filter(function () {
                //    $(this).toggle($(this).text().toLowerCase().indexOf(formData) > -1)
                //});

            },
            error: function () {
                // $("#lblMessage").html('Error while Registring the Company!.');
            }
        })
    }
    function SetId(id) {
        $('#hdnId').val(id);

    }
    function csvFileUpload() {

        var rowCount = 0;
        var regex = /^([a-zA-Z0-9\s_\\.\-:])+(.csv|.txt)$/;
        if (regex.test($("#fileUploadRIMaster").val().toLowerCase())) {

            if (typeof (FileReader) != "undefined") {
                var reader = new FileReader();
                reader.onload = function (e) {

                    var rows = e.target.result.split('\r\n');

                    for (var i = 0; i < rows.length; i++) {

                        var cells = rows[i].split(",");
                        var RRC = rowCount + 1;
                        if (i == 0) {


                            // Heater validation
                            if (cells.length == 9) {

                                if (cells[0] != "Name") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check column name mismatch (Name).");
                                    return false;
                                }
                                if (cells[1] != "Category") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check column name mismatch (Category).");
                                    return false;
                                }
                                //if (cells[2] != "Description") {
                                //    $('#fileUploadRIMaster').val('');
                                //    alert("Please check column name mismatch (Description).");
                                //    return false;
                                //}
                                if (cells[3] != "RegularPrice") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check column name mismatch (RegularPrice).");
                                    return false;
                                }
                                if (cells[4] != "TaxType") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check column name mismatch (TaxType).");
                                    return false;
                                }
                                //if (cells[5] != "IsRecommended") {
                                //    $('#fileUploadRIMaster').val('');
                                //    alert("Please check column name mismatch (IsRecommended).");
                                //    return false;
                                //}
                                if (cells[6] != "IsActive") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check column name mismatch (IsActive).");
                                    return false;
                                }
                                if (cells[7] != "Email") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check column name mismatch (Email).");
                                    return false;
                                }
                                if (cells[8] != "Image") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check column name mismatch (Image).");
                                    return false;
                                }
                            }
                            else {
                                $('#fileUploadRIMaster').val('');
                                alert("Upload Csv file invalid template , Please check .");
                                return false;
                            }

                        } else {
                            // Row validation


                            if (cells.length == 9) {
                                if (cells[0] == "") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check Row NO : " + RRC + " Name  is empty");
                                    return false;
                                }
                                if (cells[1] == "") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check Row NO : " + RRC + " Category is empty");
                                    return false;
                                }

                                //if (cells[2] == "") {
                                //    $('#fileUploadRIMaster').val('');
                                //    alert("Please check Row NO : " + RRC + " Description  is empty");
                                //    return false;
                                //}

                                if (cells[3] == "") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check Row NO : " + RRC + " RegularPrice is empty");
                                    return false;
                                }

                                //if (cells[4] == "") {
                                //    $('#fileUploadRIMaster').val('');
                                //    alert("Please check Row NO : " + RRC + " TaxType is empty");
                                //    return false;
                                //}
                                //if (cells[5] == "") {
                                //    $('#fileUploadRIMaster').val('');
                                //    alert("Please check Row NO : " + RRC + " IsRecommended is empty");
                                //    return false;
                                //}

                                if (cells[6] == "") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check Row NO : " + RRC + " IsActive is empty");
                                    return false;
                                }
                                if (cells[7] == "") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check Row NO : " + RRC + " Email is empty");
                                    return false;
                                }
                                if (cells[8] == "") {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check Row NO : " + RRC + " Image is empty");
                                    return false;
                                }

                            }
                            else {

                                if (cells.length > 1) {
                                    $('#fileUploadRIMaster').val('');
                                    alert("Please check Row NO : " + RRC + " This row have comma simple or invalied Column length");
                                    return false;
                                }
                            }

                        }

                        rowCount += 1;

                    }

                    if (rowCount == rows.length) {
                        $("#hdnMsgStatus").val('Y');

                        $("#uploadFiles").click();

                    }

                }
                reader.readAsText($("#fileUploadRIMaster")[0].files[0]);
            } else {
                $('#fileUploadRIMaster').val('');
                alert("This browser does not support HTML5.");
                return false;
            }
        } else {
            $('#fileUploadRIMaster').val('');
            alert("Please upload a valid CSV file.");
            return false;
        }
    }

